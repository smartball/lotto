{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": [
          "./app"
        ],
        "alias": {
          "database": "./app/database",
          "repositories": "./app/repositories",
          "utils": "./app/utils",
          "routes": "./app/routes",
          "config": "./app/config"
        }
      }
    ]
  ]
}
